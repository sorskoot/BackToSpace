{
  "name": "backtospace",
  "version": "1.0.0",
  "description": "Entry for the 2019 Js13k Games competition",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start":"webpack-serve --config webpack.dev --static dist/ --client.retry",
    "start-old": "http-server ./dist -p 443 -c-1 -S -C ./c0dr_nl.pem -K ./c0dr_nl.key",
    "clean": "rm -rf dist/; mkdir -p dist",
    "buildProd": "webpack --config webpack.prod --progress --colors",
    "build": "webpack --config webpack.dev --progress --colors",
    "watch": "webpack --config webpack.dev --colors --watch",
    "zip": "cd dist; zip -r ../dist.zip .; cd ..; wc -c dist.zip",
    "package": "npm run clean && npm run build && npm run zip",
    "sass": "sass ./src/sass/root.scss ./dist/main.css",
    "sass-watch": "sass ./src/sass/root.scss ./dist/main.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sorskoot/BackToSpace.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sorskoot/BackToSpace/issues"
  },
  "homepage": "https://github.com/sorskoot/BackToSpace#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^1.0.0",
    "envify": "^4.1.0",
    "eslint": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "jshint": "^2.9.5",
    "jshint-loader": "^0.8.4",
    "node-sass": "^4.9.2",
    "sass": "^1.10.2",
    "sass-loader": "^7.0.3",
    "shader-loader": "^1.3.1",
    "stats.js": "^0.17.0",
    "style-loader": "^0.21.0",
    "transform-loader": "^0.2.4",
    "uglify-es": "^3.3.9",
    "uglify-js-es6": "^2.8.9",
    "uglifyes-webpack-plugin": "^0.4.3",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-serve": "^3.1.1"
  }
}
